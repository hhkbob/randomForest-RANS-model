application             buoyantFoam;

startFrom               latestTime;

startTime               0;

stopAt                  endTime;

endTime                 20000;

deltaT                  1;

writeControl            timeStep;

writeInterval           200;

purgeWrite              2;

timeFormat              general;

timePrecision           6;

writeFormat             ascii;

writePrecision          6;

writeCompression        off;

runTimeModifiable        yes;

maxCo        1;

maxAlphaCo        0;

